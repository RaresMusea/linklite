generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Link {
  id         String   @id @default(cuid())
  slug       String   @unique
  targetUrl  String
  createdAt  DateTime @default(now())
  ownerId    String?  // WILL BE USED LATER ON
}
