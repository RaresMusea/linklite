generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Link {
  id        String   @id @default(cuid())
  slug      String   @unique
  targetUrl String   @map("target_url")
  createdAt DateTime @default(now()) @map("created_at")
  ownerId   String?  @map("owner_id") // WILL BE USED LATER ON
}
